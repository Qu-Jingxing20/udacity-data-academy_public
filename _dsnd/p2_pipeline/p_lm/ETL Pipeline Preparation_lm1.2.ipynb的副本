    "# fac\n",
    "def load_data(messages_filepath, categories_filepath):\n",
    "    messages = pd.read_csv(messages_filepath)\n",
    "    categories = pd.read_csv(categories_filepath)\n",
    "    df = categories.merge(messages, on='id')\n",
    "    return df"
   ]
